1. 
public class Test {

	public static void main(String[] arge) {
		for (int i = 1; i <= 10; i++) {
			System.out.println(i);
		}
	}
}
2.
public class Test {
	
	public static void main(String[]arge) {

		for(int i = 10 ; i>=0; i--) {
			if(i%2==0) {
			System.out.println(i);
			}
		}
	}

}
3.
public class Test {
	
	public static void main(String[]arge) {

		for(int i = 10 ; i>=0; i--) {
			if(i%3==0) {
			System.out.println(i);
			}
		}
	}

}
